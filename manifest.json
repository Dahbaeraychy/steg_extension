{
  "manifest_version": 3,
  "name": "Steganalysis Tool",
  "description": "This extension shows an alert if an image uses Steganography or not",
  "version": "1.0",
  "background": { "service_worker": "background.js", "type": "module" },
  "permissions": ["downloads", "tabs", "scripting", "contextMenus"],
  "host_permissions": ["http://*/", "https://*/"]
}
